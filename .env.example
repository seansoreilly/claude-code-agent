# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Comma-separated list of allowed Telegram user IDs (security: only these users can interact)
TELEGRAM_ALLOWED_USERS=123456789

# Gateway server port
PORT=8080

# Claude Agent SDK settings
CLAUDE_MODEL=claude-sonnet-4-6
CLAUDE_MAX_TURNS=25
CLAUDE_MAX_BUDGET_USD=5

# Working directory for Claude operations
CLAUDE_WORK_DIR=/home/ubuntu/workspace

# Memory storage directory
MEMORY_DIR=/home/ubuntu/.claude-agent/memory
